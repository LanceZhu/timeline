{
  "name": "timeline",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "serve": "npm run serve:dev",
    "build": "npm run build:dev",
    "build:internet": "cross-env VUE_APP_SUBDOMAIN=internet vue-cli-service build",
    "build:education": "cross-env VUE_APP_SUBDOMAIN=education vue-cli-service build",
    "build:invention": "cross-env VUE_APP_SUBDOMAIN=invention vue-cli-service build",
    "build:eechina": "cross-env VUE_APP_SUBDOMAIN=eechina vue-cli-service build",
    "build:aidata": "cross-env VUE_APP_SUBDOMAIN=aidata vue-cli-service build",
    "build:edudata": "cross-env VUE_APP_SUBDOMAIN=edudata vue-cli-service build",
    "build:dev": "cross-env VUE_APP_SUBDOMAIN=dev vue-cli-service build",
    "lint": "npm run eslint && npm run stylelint",
    "lint:fix": "npm run eslint:fix && npm run stylelint:fix",
    "eslint:fix": "vue-cli-service lint",
    "eslint": "vue-cli-service lint --no-fix",
    "stylelint": "stylelint **/*.{html,vue}",
    "stylelint:fix": "stylelint **/*.{html,vue} --fix",
    "analyzer": "ANALYZER=true vue-cli-service serve",
    "release": "standard-version",
    "serve:dev": "cross-env VUE_APP_SUBDOMAIN=dev vue-cli-service serve",
    "serve:aidata": "cross-env VUE_APP_SUBDOMAIN=aidata vue-cli-service serve",
    "serve:edudata": "cross-env VUE_APP_SUBDOMAIN=edudata vue-cli-service serve",
    "serve:education": "cross-env VUE_APP_SUBDOMAIN=education vue-cli-service serve",
    "serve:internet": "cross-env VUE_APP_SUBDOMAIN=internet vue-cli-service serve",
    "serve:invention": "cross-env VUE_APP_SUBDOMAIN=invention vue-cli-service serve",
    "serve:eechina": "cross-env VUE_APP_SUBDOMAIN=eechina vue-cli-service serve"
  },
  "dependencies": {
    "@sentry/browser": "^5.17.0",
    "@sentry/integrations": "^5.17.0",
    "axios": "^0.19.2",
    "core-js": "^3.6.5",
    "cross-env": "^6.0.3",
    "dayjs": "^1.8.27",
    "element-ui": "^2.13.2",
    "lodash": "^4.17.15",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.3",
    "vue-property-decorator": "^8.4.2",
    "vue-quill-editor": "^3.0.6",
    "vue-router": "^3.2.0",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "@vue/cli-plugin-babel": "^4.3.1",
    "@vue/cli-plugin-eslint": "^4.3.1",
    "@vue/cli-plugin-typescript": "^4.3.1",
    "@vue/cli-service": "^4.3.1",
    "@vue/eslint-config-typescript": "^4.0.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-component": "^1.1.1",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^5.0.0",
    "husky": "^3.1.0",
    "quill-image-extend-module": "^1.1.2",
    "sass": "^1.26.9",
    "sass-loader": "^8.0.2",
    "standard-version": "^7.1.0",
    "stylelint": "^13.6.1",
    "stylelint-config-standard": "^20.0.0",
    "terser-webpack-plugin": "^2.3.6",
    "typescript": "~3.8.3",
    "vue-cli-plugin-typescript": "0.0.1",
    "vue-template-compiler": "^2.6.11",
    "webpack-bundle-analyzer": "^3.8.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/typescript"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "@typescript-eslint/parser"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run stylelint",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
